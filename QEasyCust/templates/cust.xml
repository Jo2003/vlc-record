<?xml version="1.0" encoding="UTF-8"?>
<customization>
  <strings>
  <!-- 
  Please note: 
  The only thing you have to make is to update the "value" part 
  of the string entries! 
  APP_NAME: the official name of the Application
  UPD_CHECK_URL: the url with update information
  API_SERVER: the API server without http://
  COMPANY_NAME: the name of the comany behind. Can be the APP_NAME
  COMPANY_LINK: http link to the companies website
  API_XML_PATH: path to XML api on server
  API_JSON_PATH: path to JSON api on server
  -->
    <string name="APP_NAME" value="[%OFF_NAME%]" />
    <string name="UPD_CHECK_URL" value="[%UPD_URL%]" />
    <string name="API_SERVER" value="[%API_SRV%]" />
    <string name="COMPANY_NAME" value="[%COMP_NAME%]" />
    <string name="COMPANY_LINK" value="[%COMP_URL%]" />
    <string name="API_XML_PATH" value="[%API_XML%]" />
    <string name="API_JSON_PATH" value="[%API_JSON%]" />
  </strings>
</customization>
